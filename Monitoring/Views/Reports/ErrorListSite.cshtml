@model IEnumerable<Monitoring.Models.education__institution>

@{
    ViewBag.Title = "Список УДО, где ошибок >=70%";
}

<div class="container" style="margin-top: 50px;">

    <a href="/Reports/Index">Вернуться к отчетам</a>
    <h4>
        Список УДО, где ошибок >=70% <br />
    </h4>

    <table>
        <thead>
            <tr>
                <th>№</th>
                <th>Наименование</th>
                <th>Область</th>
            </tr>
        </thead>
            @for (int i = 0; i < Model.Count(); ++i)
            {
                <tr>
                    <td>@(i+1)</td>
                    <td>@Model.ElementAt(i).full_name</td>
                    <td>@Model.ElementAt(i).district.area.name</td>
                </tr>
            }
        <tbody>

        </tbody>
    </table>
</div>